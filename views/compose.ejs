<%- include("partials/header"); -%>

<h1>Compose</h1>

  <form class="" action="/compose" method="post">
    <div class="form-group">
      <label for="postTitle">Title</label>
      <input type="text" name="postTitle" class="form-control top" />
    </div>
    <div class="form-group">
      <label for="postBody">Your Message</label>
      <textarea type="textarea" name="postBody" "text" cols="40" rows="5" class="form-control top"></textarea>
    </div>
    <button class="btn btn-primary" type="submit" name="button">
      Publish
    </button>
  </form>


  <%- include("partials/footer"); -%>
